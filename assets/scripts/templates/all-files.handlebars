<article>
  <div class="filter-table">
    <table class="file-table-two">
      <thead>
        <tr>
          <th>File Name</th>
          <th>Category</th>
          <th>Date Created</th>
          <th>Download</th>
        </tr>
      </thead>
      {{#each docs as |doc|}}
        <tr>
          <td>{{doc.title}}</td>
          <td>{{doc.category}}</td>
          <td>{{doc.filterDate}}</td>
          <td>
            <button class="download-btn btn btn-primary btn-lg" id="download-{{doc._id}}" type="button">
              <span class="glyphicon glyphicon-download-alt"></span>
            </button>
          </td>
        </tr>
      {{/each}}
    </table>
  </div>
</article>
