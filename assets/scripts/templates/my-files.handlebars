<aside>{{> user-message}}</aside>
<article>
  <div>
    <table class="file-table-one">
      <tr>
        <th>File Name</th>
        <th>Category</th>
        <th>Date Created</th>
        <th>Date Modified</th>
        <th>Download</th>
        <th>Edit</th>
        <th>Delete</th>
      </tr>
      {{#each docs as |doc|}}
        <tr>
          <td>{{doc.title}}</td>
          <td>{{doc.category}}</td>
          <td>{{doc.createdAt}}</td>
          <td>{{doc.updatedAt}}</td>
          <td>
            <button class="download-btn" id="download-{{doc._id}}" type="button" class="btn-sm btn-defaults">
              Download
            </button>
          </td>
          <td>
            {{!-- <a href="#"> --}}
              <button id="update-doc-open" type="button" class="btn btn-primary btn-lg" data-toggle="modal" data-target="#update-doc-modal">
                <span class="glyphicon glyphicon-pencil"></span>
              </button>
            {{!-- </a> --}}
          </td>
          <td>
            <a href="#">
              <form id="delete-doc-button">
                <button type="button" class="btn btn-primary btn-lg">
                  <span class="glyphicon glyphicon-trash"></span>
                </button>
              </form>
            </a>
          </td>
        </tr>
      {{/each}}
    </table>
  </div>
</article>
